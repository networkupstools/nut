        <CustomAction Id="SETDEFAULTPATH" Property="TARGETDIR" Value="[ProgramFilesFolder]" Execute="firstSequence" />
        <CustomAction Execute="deferred" FileKey="wdi" Id="InstallLibUSB" Return="asyncWait" HideTarget="yes" ExeCommand="" Impersonate="no" />
        <InstallExecuteSequence>
            <Custom Action="SETDEFAULTPATH" Sequence="1">TARGETDIR=""</Custom>
            <!--<Custom Action="InstallLibUSB" Before="InstallFinalize" />-->
            <Custom Action="StopService" After="MsiUnpublishAssemblies">
            </Custom>
            <Custom Action="DeRegisterNUTService" After="MsiUnpublishAssemblies">
            </Custom>
            <Custom Action="RegisterNUTService" Before="InstallFinalize" />
            <Custom Action="InstallLibUSB" Before="InstallFinalize">LIBUSBINSTALL=1</Custom>
        </InstallExecuteSequence>
