msgid "On batteries"
msgstr "Sur batteries"

#: NUT-Monitor:804
msgid "Device is running on batteries"
msgstr "Fonctionnement sur batteries"

#: NUT-Monitor:817
msgid "discharging"
msgstr "en décharge"

#: NUT-Monitor:819
msgid "charging"
msgstr "en charge"

#: NUT-Monitor:825
msgid "Model :"
msgstr "Modèle :"

#: NUT-Monitor:829
msgid "Temperature :"
msgstr "Température :"

#: NUT-Monitor:833
msgid "Battery voltage :"
msgstr "Tension batteries :"

#: NUT-Monitor:844 gui-1.3.glade.h:11
msgid "Battery charge :"
msgstr "Charge des batteries :"

#: NUT-Monitor:847 NUT-Monitor:856 NUT-Monitor:868
msgid "Not available"
msgstr "Non disponible"

#: NUT-Monitor:853
msgid "UPS load :"
msgstr "Consommation :"

