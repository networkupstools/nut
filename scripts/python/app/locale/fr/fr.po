msgid "<b>%H hours %M minutes %S seconds</b>"
msgstr "<b>%H heures %M minutes %S secondes</b>"

#: NUT-Monitor:864
msgid "<b>%M minutes %S seconds</b>"
msgstr "<b>%M minutes %S secondes</b>"

#: NUT-Monitor:866
